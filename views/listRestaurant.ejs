<table border="1">
    <thead>
        <th>ID</th>
        <th>Name</th>
        <th>Address</th>
        <th>Action</th>
    </thead>
    <tbody>
        <%restaurant.forEach(data=>{%>
            <tr>
                <td><%=data.id%></td>
                <td><%=data.name%></td>
                <td><%=data.address%></td>
                <td>
                    <a href="/restaurants/<%=data.id%>/viewmenu">View Menu</a>
                </td>
            </tr>
        <%})%>
    </tbody>
</table>