<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  </head>
  <body>
    <h1>Menu Restaurant : <%=data_rest.name%></h1>
    <form class="" action="index.html" method="post">
      <div class="form-group">
        <label for="key">Search by:</label>
          <select class="" name="">
            <%data_rest.Menus.forEach((data) =>
            {%>
              <option value="<%=data.id%>"> <%=Object.keys(data.dataValues)%> </option>
            <%})%>
          </select>
      </div>
    </form>
    <table class="table table-bordered">
      <thead>
        <tr>
          <th>Id</th>
          <th>Name</th>
          <th>Menu Type</th>
          <th>Rating</th>
          <th>Price</th>
        </thead>
        <tbody>
          <%data_rest.Menus.forEach((data, i) =>{%>
            <tr>
              <td><%=i+1%></td>
              <td><%=data.name%></td>
              <td><%=data.menu_type%></td>
              <td><%=data.rating%></td>
              <td><%= helper.ubahAgka(data.price)%></td>
            </tr>
            <%})%>
          </tbody>
  </div>
  </table>
  </body>
</html>
