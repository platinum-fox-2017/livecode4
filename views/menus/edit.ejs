  </head>
  <body>
    <h2>Add Menu</h2>
    <form action="" method="post">
      <input type="hidden" value="<%= data.id %>" name="id">
      <input type="text" value="<%= data.name %>" name="newName" placeholder="add name">
      <input type="text" value="<%= data.menu_type %>" name="newMenuType" placeholder="add menu type">
      <input type="text" value="<%= data.rating %>" name="newRating" placeholder="add rating">
      <input type="text" value="<%= data.price %>" name="newPrice" placeholder="add price">
      <input type="text" value="<%= data.RestaurantId %>" name="newResId" placeholder="add restaurant id">
      <input type="submit">
    </form>
  </body>
</html>
