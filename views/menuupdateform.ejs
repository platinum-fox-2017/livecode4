<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <p style="color:red"><%= error.err %></p>
    <form class="" action="/menus/update/<%= data.id%>" method="post">
      <table>
        <tr>
          <td>nama</td>
          <td> <input type="text" name="nama" value="<%= data.nama%>"> </td>
        </tr>
        <tr>
          <td>menu_type</td>
          <td> <input type="text" name="menu_type" value="<%= data.menu_type%>"> </td>
        </tr>
        <tr>
          <td>rating</td>
          <td> <input type="text" name="rating" value="<%= data.rating%>"> </td>
        </tr>
        <tr>
          <td>price</td>
          <td> <input type="text" name="price" value="<%= data.price%>"> </td>
        </tr>
        <tr>
          <td></td>
          <td>
            <select class="" name="restaurant_id">
              <% for(let i=0;i<restaurant.length;i++){ %>
                <% if(restaurant[i].id===data.restaurant_Id){%>
                  <option value="<%= restaurant[i].id%>" selected><%= restaurant[i].nama%></option>
                <%}else{%>
                  <option value="<%= restaurant[i].id%>"><%= restaurant[i].nama%></option>
                <%}%>
              <%}%>
            </select>
          </td>
        </tr>
        <tr>
          <td> <input type="submit" name="" value="submit"> </td>
        </tr>
      </table>
    </form>
  </body>
</html>
