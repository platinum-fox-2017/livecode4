<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Page Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" media="screen" href="main.css" />
    <script src="main.js"></script>
</head>
<body>

    <h1>Restaurant</h1>
    <table border="1">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Address</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            <%dataRes.map(val => {%>
            <tr>
                <td><%= val.id %></td>
                <td><%= val.name %></td>
                <td><%= val.address %></td>
                <td>
                    <a href="/restaurants/<%= val.id %>/view_menu">View Menu</a>
                </td>
            </tr>
            <%})%>

        </tbody>
    </table>
    
</body>
</html>