<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>MENUS</title>
    <h2>MENUS</h2>
  </head>
  <body>
    <table border="1">
      <thead>
        <td>ID</td>
        <td>Name</td>
        <td>Address</td>
        <td>Menu</td>
      </thead>
      <%dataRestaurant.forEach(data=>{%>
      <tr>
          <td><%=data.id%></td>
          <td><%=data.name%></td>
          <td><%=data.address%></td>
          <td> <a href="/restaurants/<%=data.id%>/view_menu">VIEW MENU</a></td>
        <%})%>
      </tr>
    </table>
  </body>
</html>
